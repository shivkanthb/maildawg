{
  "name": "@shivkanthb/maildawg",
  "version": "0.0.2",
  "publish": true,
  "description": "A simple service for sending beautiful responsive emails from your app.",
  "env": [
    {
      "name": "nodemailer_source",
      "defaultValue": "",
      "description": "Examples - Gmail, Zoho, Yahoo etc"
    },
    {
      "name": "nodemailer_host",
      "defaultValue": "",
      "description": "Examples - smtp.gmail.com, smtp.zoho.com"
    },
    {
      "name": "nodemailer_user",
      "defaultValue": "",
      "description": "user@domain.tld"
    },
    {
      "name": "nodemailer_pass",
      "defaultValue": "",
      "description": "Password for the provided email address"
    }
  ]
}